{
  "type": "template",
  "slug": "saas-starter",
  "name": "SaaS Starter Template",
  "description": "Full-stack SaaS application with authentication, billing, and dashboard",
  "tags": ["saas", "nextjs", "stripe", "auth", "supabase"],
  "requiredEnv": [
    {
      "name": "STRIPE_SECRET_KEY",
      "description": "Stripe secret key for payment processing",
      "example": "sk_test_..."
    },
    {
      "name": "NEXT_PUBLIC_SUPABASE_URL",
      "description": "Supabase project URL",
      "example": "https://xxx.supabase.co"
    },
    {
      "name": "SUPABASE_SERVICE_KEY",
      "description": "Supabase service role key",
      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
    }
  ],
  "view": [
    { "path": "README.md", "lines": "all" },
    { "path": "src/app/page.tsx", "lines": [1, 50] },
    { "tree": { "path": "src", "depth": 2, "limit": 20 } }
  ]
}